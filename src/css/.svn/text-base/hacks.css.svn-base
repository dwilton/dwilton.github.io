/* #endregion */input[type="image"] {
  padding: 0;
  background: none;
  border: none;
}
span.placeholder {
  position: absolute;
  padding: 2px;
  font-size: 12px;
  font-family: Arial, Helvetica, sans-serif;
  color: #787878;
  cursor: text;
  z-index: 1;
}
span.placeholder-hide-except-screenreader {
  padding: 0 !important;
  width: 1px !important;
  height: 1px !important;
  border: 0 !important;
  clip: rect(1px 1px 1px 1px);
  clip: rect(1px, 1px, 1px, 1px);
  overflow: hidden;
}
span.placeholder-hide {
  display: none;
}
label.visuallyhidden-with-placeholder {
  height: auto !important;
  position: absolute !important;
  left: -999em;
  overflow: visible !important;
}
/* FIXME: this doesn't belong in the style guide, 
     it should sit in the application or become generic 
     search component */
.accountSearch input,
.accountSearch select {
  border-radius: 0;
}
button span {
  color: #555555;
}
button.Processing .ProcessIndicator .ProcessText {
  color: #bbbbbb;
}
button#copyButton:hover span,
button#copyButton:focus span,
button#copyButton:active span {
  color: #ffffff;
}
button:hover span,
button:focus span {
  color: #ffffff;
}
button.DesiredAction span {
  color: #ffffff;
}
button.Delete span {
  color: #ffffff;
}
.headingOne {
  margin: 0 0 10px;
  padding: 0;
  font: bold 26px/32px Arial, Helvetica, sans-serif;
  color: #333333;
}
.headingTwo,
.headingThree,
.headingFour,
.headingFive,
.headingSix {
  margin: 0 0 10px;
  padding: 0;
  font: bold 18px/24px Arial, Helvetica, sans-serif;
  color: #333333;
}
.headingFour,
.headingFive,
.headingSix {
  font-weight: normal;
}
.accountSearch label {
  color: #383838;
}
.DataErrorBox {
  font-size: 12px;
  text-align: left;
}
fieldset.Border .SegmentedButtons legend,
fieldset.Border .Filters .SegmentedButtons legend {
  position: absolute;
  left: -9999em;
}
/* FIXME: No idea what this is? I'm guessing its used to overide some rubbish CSS from CORE */
ul.tabs {
  padding: 5px 12px 0 10px;
}
ul.tabs li {
  display: inline-block;
  float: none;
  padding: 6px 10px;
  margin: 0 5px 0 0;
  border: 2px solid transparent;
  border-bottom: none;
}
.pageSubContent .TabContent {
  padding: 15px;
  overflow: hidden;
}
.pageSubContent .TabContainer {
  padding: 15px;
}
.pageSubContent .TabContent,
.pageSubContent .Tabs li.Active {
  background-color: #f6f6f6;
  /* FIXME: Shouldn't have hardcoded colors */

}
.lightboxContainer .TabContent,
.lightboxContainer .Tabs li.Active {
  background-color: #ebebeb;
  /* FIXME: Shouldn't have hardcoded colors */

}
.error-container {
  position: relative;
}
.error-container.focused span.error {
  display: inline-block;
}
.error-container input,
.error-container select,
.error-container textarea {
  float: none !important;
  border-color: #db0000 !important;
}
.error-container input:focus + span.error,
.error-container select:focus + span.error,
.error-container textarea:focus + span.error {
  display: inline-block;
}
.error-container #cc-icon,
.error-container .cc-icon {
  display: none;
}
span.error {
  display: block;
  position: absolute;
  top: 0;
  min-width: 150px;
  max-width: 270px;
  border-width: 2px;
  border-style: solid;
  border-radius: 3px;
  z-index: 10001;
  pointer-events: none;
  background-clip: padding-box;
  transition-property: opacity;
  transition-duration: 0.2s;
  transition-timing-function: ease-out;
  /* Top */

  /* Right */

  /* Bottom */

  /* Left */

  border-color: #e33a3a;
  display: none;
  top: auto;
  left: auto;
  float: none;
  margin: -5px 0 0 0;
  max-width: 800px;
  /* bleh */

  white-space: nowrap;
}
span.error .Content {
  display: block;
  position: relative;
  padding: 10px 12px;
  font-weight: bold;
  line-height: normal;
  z-index: 3;
}
span.error .Icon {
  display: inline-block;
  position: absolute;
  top: 8px;
  bottom: 10px;
  left: 10px;
  width: 35px;
  max-height: 35px;
  z-index: 4;
}
span.error .Icon span {
  display: inline-block;
  position: absolute;
  top: 50%;
  margin-top: -11px;
  width: 26px;
  height: 26px;
}
span.error .Arrow,
span.error .Arrow span {
  position: absolute;
  border-style: solid;
  border-width: 11px;
  height: 0;
  width: 0;
  z-index: 1;
}
span.error.top {
  margin-top: -32px;
}
span.error.top .Arrow {
  top: auto;
  left: 50%;
  margin-left: -10px;
}
span.error.top .Arrow span {
  top: -14px;
  left: 50%;
  margin-left: -11px;
}
span.error.right {
  margin-left: 12px;
}
span.error.right .Arrow {
  top: 50%;
  left: -22px;
  margin-top: -10px;
}
span.error.right .Arrow span {
  top: -11px;
  left: -8px;
}
span.error.bottom {
  margin-top: 12px;
}
span.error.bottom .Arrow {
  top: -22px;
  left: 50%;
  margin-left: -10px;
}
span.error.bottom .Arrow span {
  top: -8px;
  left: 50%;
  margin-left: -11px;
}
span.error.left {
  margin-left: -12px;
}
span.error.left .Arrow {
  top: 50%;
  right: -22px;
  margin-top: -10px;
}
span.error.left .Arrow span {
  top: -11px;
  left: -14px;
}
span.error .Content {
  color: #000000;
  background: #fff5f5;
}
span.error .Icon span {
  background: url("data:image/png;base64,") no-repeat 0 0;
}
span.error .Arrow span {
  display: block;
  position: absolute;
  width: 0;
  height: 0;
}
span.error.top .Arrow {
  border-color: #e33a3a transparent transparent transparent;
}
span.error.top .Arrow span {
  border-color: #fff5f5 transparent transparent transparent;
}
span.error.right .Arrow {
  border-color: transparent #e33a3a transparent transparent;
}
span.error.right .Arrow span {
  border-color: transparent #fff5f5 transparent transparent;
}
span.error.bottom .Arrow {
  border-color: transparent transparent #e33a3a transparent;
}
span.error.bottom .Arrow span {
  border-color: transparent transparent #ffffff transparent;
}
span.error.left .Arrow {
  border-color: transparent transparent transparent #e33a3a;
}
span.error.left .Arrow span {
  border-color: transparent transparent transparent #fff5f5;
}
span.error.left {
  left: -100%;
  min-width: 100%;
  margin-left: -22px;
}
span.error.right {
  margin-left: 12px;
}
span.error .Content {
  font-weight: normal;
  font-size: 12px;
}
