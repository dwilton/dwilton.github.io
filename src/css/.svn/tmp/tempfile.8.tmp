a.Button,
button {
  color: #555555;
  border: 0;
  border-bottom-width: 2px;
  border-style: solid;
  border-color: #d9d9d9;
  background: #f3f3f3;
  transition: all 0.1s ease;
}
a.Button:focus:enabled,
button:focus:enabled,
a.Button:hover:enabled,
button:hover:enabled,
a.Button:active:enabled,
button:active:enabled {
  color: #ffffff;
  background: #ff4526;
  border-color: #bf1b00;
}
a.Button:disabled,
button:disabled {
  color: #bbbbbb;
  background-color: #d9d9d9;
  border-color: #a6a6a6;
}
a.Button.Processing .ProcessIndicator .ProcessIcon,
button.Processing .ProcessIndicator .ProcessIcon {
  background: url("data:image/gif;base64,") no-repeat 50% 50%;
  background-position: center center;
  opacity: 0.5;
}
a.Button:link,
a.Button:visited,
a.Button:focus,
a.Button:active {
  color: #555555;
}
a.Button.Active:link,
a.Button.Active:visited,
a.Button.Active:focus,
a.Button.Active:active {
  color: #ffffff;
  background-color: #50526b;
  border-color: #242531;
}
a.Button:hover {
  color: #ffffff;
  background: #ff4526;
  border-color: #bf1b00;
  text-decoration: none;
}
.Alert,
