/**
   * Containers
   */
.Tabs li {
  color: #999999;
  background-color: transparent;
  border: 1px solid #ebebeb;
  border-bottom: 0;
}
.Tabs li:after {
  border-top: 3px solid transparent;
}
.Tabs li a {
  color: #999999;
}
.Tabs li a:hover {
  color: #333333;
}
.Tabs li.Active {
  background-color: #ffffff;
}
.Tabs li.Active:before {
  background: linear-gradient(to right, #ff4526 0%, #ff8500 100%);
  background-color: #ff4526;
}
.Tabs li.Active a {
  color: #333333;
  text-decoration: none;
  cursor: pointer;
}
.TabsVertical .Tabs li {
  border: 1px solid #ebebeb;
}
.TabsVertical .Tabs li.Active {
  border-right: 1px solid #ffffff;
}
.TabsVertical .Tabs li .TabContent {
  border-top: 0;
  border-left: 1px solid #ebebeb;
  background-color: #ffffff;
}
.TabContent {
  border-top: 1px solid #ebebeb;
  background-color: #ffffff;
}
.Invalid {
  border-color: #db0000 !important;
}
input[data-val-required].Invalid,
select[data-val-required].Invalid,
textarea[data-val-required].Invalid {
  box-shadow: inset 1px 1px 3px rgba(0, 0, 0, 0), 0 1px rgba(255, 255, 255, 0.85), -3px 0 #db0000;
}
