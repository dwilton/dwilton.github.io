/* #region Lozenge Control */

.LozengeControl() {

  .Lozenge {
    display: inline-block;
    overflow: hidden;
    width: 100px;
    height: 41px;
    padding: 4px 3px;
    margin: 0;
    font-size: 12px;
    line-height: 17px;
    text-align: center;
    text-overflow: ellipsis;
    vertical-align: middle;
    color: #404040;
    .UserSelect(none);

    > span {
      display: table;
      width: 100%;
      height: 100%;

      span {
        display: table-cell;
        vertical-align: middle
      }
    }
  }

}

/* #endregion */

/* #region Lozenge Style */

.LozengeStyle() {

  .Lozenge {
    background: transparent url('../img/components/UI/sprite-lozenges.png') no-repeat -649px -60px;

    &.Has {
      background-position: -649px 0;
    }

    &.Essentials,
    &.Seniors,
    &.RSP,
    &.BuildYourOwn {
      color: #e92324;
      background-position: 0 -60px;
    
      &.Has {
        background-position: 0 0;
      }
    }

    &.MoviesBasic,
    &.MoviesDrama {
      color: #00bcf2;
      background-position: -108px -60px;
    }

    &.MoviesBasic.Has {
      background-position: -541px 0;
    }

    &.MoviesDrama.Has {
      background-position: -108px 0;
    }

    &.Sport {
      color: #8dc644;
      background-position: -216px -60px;

      &.Has {
        background-position: -216px 0;
      }
    }

    &.KidsMusic,
    &.DramaLifestyle,
    &.DocumentaryAdventure {
      color: #5f2b85;
      background-position: -325px -60px;
   
      &.Has {
        background-position: -325px 0;
      }
    }

    &.Hd,
    &.HD,
    &.Adult {
      background-position:-433px -60px;
      color:#000;

      &.Has {
        background-position: -433px 0;
      }
    }

    &.Hd,
    &.HD {
      font-size: 16px;
    }

    &.Offer {
      background-position: -541px -60px;
      color: #000;

      &.Has {
        color: #000;
      }
    }

    &.MiniPack {
      background-position: -757px -60px;

      &.Has {
        background-position: -757px 0;
      }
    }

    &.Box {
      background-position: -433px 0;

      span span {
        text-align: left;
        text-indent: -9999em;
        background: transparent none no-repeat center center;
      }

      &.Iq span span {
        background-image: url('data:image/png;base64,');
      }

      &.IqHd span span {
        background-image: url('data:image/png;base64,');
      }

      &.Mystar span span {
        background-image: url('data:image/png;base64,');
      }

      &.MystarHd span span {
        background-image: url('data:image/png;base64,');
      }

      &.NoFunc {
        position:relative;

        &:after {
          content: 'No Func.';
          display: block;
          position: absolute;
          bottom: 2px;
          left: 0;
          width: 100%;
          color: #fff;
          font-size: 10px;
          text-align: center;
        }

        span span {
          background-position:center 2px;
        }
      }
    }

    &.Has {
      color: #fff;
    }

    &.Magazine {
      text-align: left;
      text-indent: -9999em;
      background: transparent url('data:image/png;base64,') no-repeat center center;

      > span {
        display: none;
      }
    }
  }

}

/* #endregion */