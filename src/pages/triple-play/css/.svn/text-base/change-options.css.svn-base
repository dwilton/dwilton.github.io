/* START links section Component */

.DetailsSection{
  padding: 0 15px;
  margin: 15px 0px;
}
.DetailsSection.LeftBorder{
  border-left: 1px solid #adadad;
  min-height: 208px;
}
.DetailsSubSection{
  margin:5px 0 0 0;  
}
.DetailsSection h6{
  margin:0;
}
.ToolsContainer{
  position: absolute;
  right: 0px;
  margin: 7px 20px 7px 0px;
  z-index: 1;
}
.ToolsContainer a{
  margin-right:10px;
  font-weight:bold;
}
.ToolsContainer a:last-child{
  margin-right:0px;
}
.ToolsContainer li{
  display: inline-block;
  margin-left: 15px;
}
/* END links section Component */
/* START Multi Selector Component */

.MultiSelector a, .MultiSelector a:link, .MultiSelector a:visited, .MultiSelector a:focus, .MultiSelector a:hover, .MultiSelector a:active {
  font-weight: normal;
}
.MultiSelector .SelectorsContainer {
  overflow: auto;
  clear: both;
  max-height: 385px;
  background-color: #fff;
  border: 2px solid #adadad;
}
.MultiSelector#specialtyTiersContainer .SelectorsContainer {
  height: 405px;
}
.MultiSelector .SelectorsContainer.Magazines {
  overflow: hidden;
  height: auto;
  margin: 0 0 20px;
}
.MultiSelector .SelectorsContainer:after {
  content: '.';
  display: none;
  clear: both;
  height: 0;
}
.MultiSelector.Flexible .SelectorsContainer {
  /* overflow:visible; This looks better for the component, but puts it out of alignment with other instances on the page */
  height: auto;
}
.MultiSelector.Boxes .SelectorsContainer {
  height: 244px;
}
.MultiSelector .SelectorPanel {
  float: left;
  display: inline-block;
  width: 50%;
  vertical-align: top;
}
.MultiSelector .SelectorsContainer .Choices {
  border-right: 2px solid #adadad;
}
.MultiSelector li {
  position: relative;
  overflow: hidden;
  height: 56;
  padding: 20px 125px 18px 15px;
  border-top: 1px solid #cfcfcf;
}
.MultiSelector .Selected li {
  padding: 18px 45px 20px 95px;
}
.MultiSelector li:first-child {
  border-top: none;
}
.MultiSelector .SelectorPanel li.Placeholder {
  padding: 0px;
  line-height: 56px;
  text-align: center;
}
.MultiSelector li.Placeholder div {
  font-size: 12px;
  color: #888;
  background-color: #eee;
}
.MultiSelector .Selector {
  display: block;
  position: absolute;
  top: 14px;
  right: 25px;
  text-align: left;
  text-indent: -9999em;
  cursor:pointer;
  width: 45px;
  height: 31px; 
  background: transparent url('data:image/png;base64,') no-repeat 0 0;
}
.MultiSelector .Selected .Selector {
  right: auto;
  left: 25px;
  background-image: url('data:image/png;base64,');
}
.MultiSelector li.Placeholder .Selector {
  display: none;
}
.MultiSelector li select, .MultiSelector li label {
  margin-top: 7px;
  background-color:#F1F1F1;
  font-weight:700;
}
.MultiSelector li p select, .MultiSelector li p label {
  background-color:transparent;
}
.MultiSelector li p.description select, .MultiSelector li p.description label {
  margin-top: 0;
  color: #666;
}
#extrasContainer li select {
  max-width:265px;
}
#accessories #extrasContainer li select {
  max-width:265px;
}
.MultiSelector li label {
  display: inline-block;
  font-weight: normal;
}
.MultiSelector li img {
  position:relative;
}
.MultiSelector p.description {
  margin-top: 7px;
  color: #666;
}

.MultiSelector .Extras li {
  padding-top:12px;
  padding-bottom:12px;
}
.MultiSelector .ExtrasImage {
  display:none;
  float:left;
  width:100px;
  height:80px;
  padding:0;
  margin:-12px 12px 0 0;
  text-align:center;
}
.MultiSelector .ExtrasImage img {
  top:4px;
}
.MultiSelector .ExtrasImage.visible {
  display:block;
}

/* START Selector Panel Description Description */
.SelectorPanel .Description {
  display: inline-block;
  width: auto;
  max-width: 325px;
  padding: 0 0 0 12px;
  vertical-align: top;
}
.SelectorPanel .Description p {
  padding: 0;
  margin: 0;
  line-height: 14px;
}
.SelectorPanel .Description a {
  line-height: 20px;
}
.SelectorPanel .Description .price {
  position: absolute;
  top: 33px;
  right: 80px;
  font-weight: bold;
  font-size: 16px;
  line-height: 16px;
}
.Selected .SelectorPanel .Description .price {
  right: 15px;
}
.BoxType {
  display: inline-block;
  width: 100px;
  vertical-align: top;
  font-weight: bold;
}
/* END Selector Panel Description Description */
/* END Multi Selector Component */
/* Drop Down Style OverRides  */

.OptionSelect.FullWidth{
  width: 100%;
  background-image: url(data:image/png;base64,);
  padding: 7px;
  border: 2px solid #adadad;
  border-bottom: none;
}
.ServiceSelect{
  width: 160px;
}
.ServiceSelect option{
  text-align: right;
}

/* START TabInfoBar Component */
.TabInfoBar{
  margin: 5px 0px;
}
.TabInfoBar .rightAlign{
  padding-right: 0px
}
.TabInfoBar .select2-choice > .select2-chosen{
  font-weight: bold;
  color: #FF5420;  
}
.ContractSelection {
  width: 230px;
}
.select2-container.Stealth .select2-choice,
.select2-container.Stealth .select2-choice .select2-arrow{
  border: 1px solid transparent;
  background: none;
}
.Stealth.select2-dropdown-open .select2-choice {
  border: 1px solid #5897fb;
  border-bottom: none;
}
/* END TabInfoBar Component */
/* START Basket Style */

.Basket {
  position: relative;
  padding: 0 0 0px;
  margin: 78px 0 0;
  border: 1px solid #d6d6d6;
  border-bottom: none;
}

.Basket .BasketHeader {
  background: transparent url('data:image/png;base64,') repeat-x 0 0;
  border-bottom: 1px solid #d6d6d6;
}
.Basket .monthlyCharges h4 {
  border-bottom: 1px solid #d6d6d6;
}
.Basket .BasketContent{
  background: #ffffff;
  max-height: 461px;
  overflow-y: auto;
}
.Basket .BasketContent h4 {
  margin: 0 15px;
  padding: 12px 0px 8px;
}
.Basket table {
  width: 100%;
  border-collapse: collapse;
}
.Basket th, .Basket td {
  padding: 0 18px;
}
.Basket th, .Basket td.BasketTotal {
  padding-bottom: 4px;
  font-weight: bold;
  font-size: 14px;
}
.Basket tbody th {
  padding-top:16px;
}
.Basket tbody tr:first-child th {
  padding-top:4px;
  padding-left:0px;
}
.Basket td.BasketTotal {
  position: relative;
  text-align: right;
}
.Basket td.BasketTotal:before {
  content: '';
  position: absolute;
  top: 0;
  right: 12px;
  display: block;
  width: 65px;
  height: 1px;
  background-color: #ccc;
}
.Basket td:first-child + td {
  width: 80px;
  padding-left: 0;
  text-align: right;
}
.Basket tfoot tr:first-child td,
.Basket .OneOffTotal {
  background: transparent url('data:image/png;base64,') repeat-x 0 0;
}
.Basket tfoot tr:first-child td {
  padding-bottom: 28px;
  background-position:0 center;
}
.Basket .OneOffTotal {
  padding-top: 8px;
  margin-top: 8px;
}
.Basket tfoot td {
  text-align: right;
}
.monthlyTotalsSection {
  clear: both;
  margin: 0 0px;
  border-top:solid 1px #d6d6d6;
  border-bottom:solid 1px #d6d6d6;
  background-color: #F5F5F5;
  padding-bottom: 15px;
}
.monthlyTotalsSection td{
  padding: 4px 18px;
}
.Basket .OneOff:after,
.OneOffTotal:after {
  content: '';
  display: block;
  clear: both;
  height: 0;
  visiblity: hidden;
}

.BasketWarning {
  padding:5px 15px 10px;
  color:#d40100;
}
.removedOffer {
  color:#d40100;
}
.additionalCost{
  color:#009743;
}
.monthlyCharges{
  height: 233px;
  overflow-y: auto;
}
.monthlyChargeGroup{
  margin: 10px 0px;
}
.monthlyCharges table{
  margin: 10px auto;
  font-size: 12px;
}
.BasketContent h6{
  font-size: 14px;
  font-weight: bold;
  margin: 10px 0 0 17px; 
}
.monthlyCharges tbody td:first-of-type:before{
  content: '';
  display:inline-block;
  width:13px;
  height:13px;
  background: transparent url('data:image/png;base64,') no-repeat 0 2px;
}
.monthlyCharges tbody .removedOffer td:first-of-type:before{
  background: transparent url('data:image/png;base64,') no-repeat 0 2px;
}
/* CSS got out of hand a bit, needs refactor */
.Basket .NewTotal {
  position: relative;
  float: right;
  display: inline-block;
  height: 29px;
  padding: 0 85px 0 5px;
  margin: 8px 12px 18px 0;
  line-height: 29px;
  color: #fff;
  background: transparent url('data:image/png;base64,') repeat-x 0 0;
}
.Basket .NewTotal:after, .Basket .NewTotal span {
  position: absolute;
  display: block;
  top: 0;
  width: 5px;
  height: 29px;
  background-color: transparent;
  background-repeat: no-repeat;
  background-position: 0 0;
}
.Basket .NewTotal:after {
  content: '';
  left: -5px;
  background-image: url('data:image/png;base64,');
}
.Basket .NewTotal span {
  right: -5px;
  width: 80px;
  padding: 0 12px 0 0;
  font-weight: bold;
  text-align: right;
  color: #fff;
  background-image: url('data:image/png;base64,');
  background-position: right top;
}
.Basket .OneOff .NewTotal {
  margin-bottom: 0;
}
.BasketTotal{
  font-weight: bold;
  font-size: 14px;
}
.BasketTotal td{
  padding-top: 15px;
}
.ActionButtons .Secondary{
  position: absolute;
}
/* END BASKET */



/*  Start BREADCRUMBS  */
.Breadcrumb{
  display: inline-block;
  font-size: 12px;
}
.Breadcrumb li{
  display: inline-block;
}
.Breadcrumb li:not(.CurrentCrumb) span{
  cursor: pointer;
}
.Breadcrumb li:before{
  content: ">";
  text-decoration: none;
}

/*  End BREADCRUMBS  */


/* ad Hoc Styling */
/* over ride */
.HiddenInputPadding,.RightPadding, .LeftPadding, .NoPadding {padding:0}
.RightPadding {padding-right:10px;}
.LeftPadding {padding-left:10px;}

.DetailsSection .SelectDropDown{
  width: 99%; 
}
.DetailsSection .FixedInputWidth{
  width: 94%;
}
/*Phone Tab Styling */

.AlternateProviderNumberInputs,
.ModemSelect,
.NewNumberInputs{
  display: none;  
}

.SelectorPanel .SelectDropDown{
  margin-top:-6px;
  margin-bottom:-6px;
}
.HiddenInputSpacing{
  margin: -10px 0px 10px 27px;
  font-size: 12px;
}
.HiddenInputSpacing label{
  margin: 10px 0px -1px;
}
.HiddenInputSpacing label.SelectLabel{
  margin-bottom: 5px;
}
.HiddenInputSpacing.FixSelectSpacing{
  margin-top: 2px;
}
label.TopLabel{
  display: block;
}

.HorizontalLabel label,
label.HorizontalLabel{
  display:inline-block;
}

.HorizontalLabel label.VerticalLabel{
  display:block;
}
.DetailsSubSection textarea{
  width:97%;
  height: 81px;  
}
.smallInput{
  min-width: 185px
}
.SelectDropDown.smallInput{
  min-width: 200px
}