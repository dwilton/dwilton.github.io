/**
 * Disconnected
 */

.disconnected .columns .columns{
  margin: 20px 0 0;
}

.disconnected h2 {
  font:normal 24px "Proxima Nova Light", Arial, Helvetica, Serif;
}

.disconnected h3 {
  margin: 0;
  font-size: 16px;
  color: #555
}

.disconnected .Alert {
  font: normal 22px "Proxima Nova Light", Arial, Helvetica, Serif;
}

.disconnectedDetails.columns {
  padding: 0 30px 0 0;
}

.disconnectedDetails .block-grid {
  margin-bottom: 30px;
}

.disconnectedDetails .block-grid h3 {
  margin-bottom: 5px;
}

.disconnectedCharges ul {
  margin-bottom: 15px;
}
.disconnectedCharges ul li {
  margin:0;
}

.disconnectedCharges ul li:first-child {
  margin-bottom: 4px;
  padding:5px 0 4px;
  border-bottom: solid 1px #eee;
}

.disconnectedCharges ul li:first-child label {
  font: normal 14px "Proxima Nova semiBold", Arial, Helvetica, Serif;
}

.disconnectedCharges label {
  display: inline-block;
  margin:0;
}

/**
 * Theming, Colours and Gradients
 */

.greyTitle, .alertTitle {
  margin: 0;
  padding: 7px 10px;
  font-weight: normal;
  font-size: 22px;
  color: #fff;
}

.greyGradient, .greyTitle {
  color: #fff;
