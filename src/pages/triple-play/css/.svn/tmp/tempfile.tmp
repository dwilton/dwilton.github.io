/**
 * Common
 */

html,
button,
input,
select,
textarea {
  color: #222;
}

html {
  min-width: 1340px;   
  font-size: 1em;
  line-height: 1.4;
}

body {
  height: 100%;
  font-family: 'Proxima Nova Regular', Arial, Helvetica;
  font-size: 14px;
  color: #555;
}

h5 label {
  font-weight: normal;
  font-size: 13px;
  color: #999;
}

h5 span {
  display: block;
  font-size: 16px;
  line-height: 18px;
  color: #555;
}

h5 span.inlineGrey {
  color: #999;
}

.semiBold {
  font-family: 'Proxima Nova SemiBold';
}

.regular {
  font-family: 'Proxima Nova Regular';
  font-weight: normal;
}

.light {
  font-family: 'Proxima Nova Light';
  font-weight: normal;
}

a.openTip {
  padding-right: 20px;
  color: #999;
  background: url(../img/icon_info_small.png) no-repeat right center;
}

.lightGreyBg {
  background: #ececec;
}

.lightGreyBg .padded {
  padding: 10px;
}

.totals {
  font-size: 24px;
}


/* Fit Text classes */

.fitText16 {
  display: block;
  height: 30px;
}


/* JS Specific CSS */

.collapsed {
  width: 0% !important;
  overflow: hidden;
}

.shiftLeft {
  margin-left: -101%;
}

/* Pane */

.pane li label {
  display: inline-block;
  padding: 0;
  width: 75%;
  font-weight: normal;
  vertical-align: bottom;
  white-space: nowrap;
  overflow: hidden;
  -ms-text-overflow: ellipsis;
   -o-text-overflow: ellipsis;
      text-overflow: ellipsis;
}

.pane li .right {
  position: relative;
  background: #fff;
  z-index: 100;
}

.pane.tiers label {
  width: 63%;
  margin-bottom:0;
}





/**
 * Side Nav
 */

#sideNav {
  position: fixed;
  width: 45px;
  height: 100%;
  white-space: nowrap;
  color: transparent;
  background :#3d3d3d;
  z-index: 99;
  overflow: hidden;
}

#sideNav.expanded {
  width: 200px;
}

#sideNav > a, 
#sideNav > span {
  display: block;
  margin: 20px 0;
  padding-left: 45px;
  height: 36px;
  font: 16px 'Proxima Nova SemiBold', Arial, Helvetica;
  line-height: 36px;
  color: #fff;
  background-image: url(../img/sprite_sidenav.png);
  background-repeat: no-repeat;
}

#sideNav .disabled, 
.inActive  {
  opacity: 0.5;
  filter: alpha(opacity=50);
}

#sideNav a span {
  display: inline-block;
  margin-left: 5px;
  color: #aaa;
}

#sideNav_search {
  background-position: 10px 6px;
}

#sideNav_summary {
  background-position: 10px -43px;
}

#sideNav_sales {
  background-position: 9px -97px;
}

#sideNav_hierarchy {
  position: absolute;
  bottom: 0;
  background-position: 10px -150px;
}

#sideNav_logout {
  background-position: 10px -208px;
}

#sideNav_telco {
  background-position: 10px -208px;
}

#sideNav_info {
  background-position: 10px -208px;
}




/**
 * Main Layout
 */

#triplePlay {
  display: table;
  padding-left: 45px;
  width: 100%;
  height: 100%;
  background: #fff;
}




/**
 * Account Profile
 */

#accountProfile {
  display: table-row;
  min-height: 250px;
  height: 27%;
}

#accountProfileTop {
  background: url(data:image/png;base64,);
}

#accountProfileTop .row {
  max-height: 75px;
  overflow: hidden;
}




/**
 * Account Info
 */

#accountProfileTop .AccountholderName {
  margin: 10px 10px 0 0;
  padding: 0;
  height: 50px;
  font-family: 'Proxima Nova Light'; 
  font-size: 38px;
  line-height: 100%;
  letter-spacing: -1px;
}

#accountProfileTop ul, #accountProfileSecondary ul {
  margin: 0;
  padding: 0;
  overflow: visible;
}

#accountInfo li {
  padding: 0;
  width: 14%;
  text-align: center;
}

#accountInfo li .icon {
  display: block;
  margin-bottom: 2px;
  width: 100%;
  height: 25px;
  background-position: center center;
  background-repeat: no-repeat;
}

#accountInfo li label {
  font: normal 16px 'Proxima Nova Regular', Arial, Helvetica;
  color: #999;
}

#accountInfo li h4 {
  margin: 0 0 2px;
  padding: 0;
  height: 25px;
  font: normal 20px/30px 'Proxima Nova Regular', Arial, Helvetica;
}

#accountInfo li .accountInfo_noContract {
  background: url(../img/sprite_tripleplay.png) no-repeat center -188px;
}

#accountInfo li .accountInfo_contract {
  position: relative;
  font-size: 11px;
  line-height: 40px;
  text-align: center;
  color: #fff;
  background: url(../img/sprite_tripleplay.png) no-repeat center -149px;
}




/**
 * Account Profile Secondary
 */

#accountProfileSecondary {
  height: 175px;
}

#accountProfileSecondary h5 label {
  margin: 0;
}

#accountProfileSecondary li > h5:first-child {
  height: 65px;
}

#accountProfileSecondary .buttons {
  white-space: nowrap;
}

#accountProfileSecondary .buttons a {
  font-size: 16px;
  margin-right: 10px;
  text-decoration: underline;
}


/* Telstra Customer */

.TelstaCustomer {
  margin-top: 30px;
  padding: 15px 15px 15px 55px;
  font-size: 16px;
  background: #e7eef4 url(../img/icon_info.png) no-repeat 15px 15px;
}


/* Authorized Users */

.authorizedUsers {
  overflow: visible;
}

.authorizedUsers > h5 {
  min-width: 250px;
}

.authorizedUsers {
  white-space:nowrap;
}

.authorizedUsers .userDOB {
  float: right;
  font: normal 14px 'Proxima Nova Regular', Arial, Helvetica, serif;
  color: #999;
}


/* Billing */ 

#billingInfo ul {
  overflow: hidden;
}


/* Soft Phone */

.softPhone {
  background: url(../img/ui_softphone_bg.png) repeat-x left top;
}

.softPhone address {
  margin-top: 5px;
  padding-left: 60px;
  height: 38px;
  font: normal 16px/38px 'Proxima Nova semiBold', Arial, Helvetica, Serif;
  background: url(../img/sprite_softphone.png) no-repeat left top;
  border: solid 1px #ebebeb;
}

.softPhone h6 {
  margin: 0;
  padding: 2px 8px;
  font-size: 14px;
  color: #fff;
  background: #d37bce;
}

.softPhone p {
  margin: 0 0 12px;
  padding: 4px 8px;
  font-size: 12px;
  line-height: 1.2;
  border: solid 1px #f2d7f0;
  border-top: 0;
}

.softPhone.outBound address {
  background-position: left bottom;
}

.softPhone h5 {
  margin-bottom: 18px;
  padding: 5px 15px 10px;
  background: url(data:image/png;base64,);
}

.softPhone h5 label, .softPhone h5 span {
  font: normal 14px/16px 'Proxima Nova Regular', Arial, Helvetica, serif;
}

.softPhone h5 label {
  display: inline-block;
  margin-bottom: 4px;
}

.softPhone h5 span {
  font: normal 18px/20px 'Proxima Nova SemiBold', Arial, Helvetica, serif;
}





/**
 * Content Area
 */

#contentArea {
  display: table-row;
  background: #ececec url(../img/ui_content_shadow_bg.png) top left repeat-x;
}

#ContentWrapper {
  padding: 20px 0 10px;
  overflow: hidden;
}





/**
 * Account Summary
 */

/* Navigation */

.mainNav li a, .mainNav li span {
  display: block;
  margin-left: 5px;
  padding: 10px 0 10px 20px;
  font-size: 14px;
  text-transform: uppercase;
  color: #666;
  background: url(../img/ui_nav_arrow.png) no-repeat left 13px;
  border-bottom: solid 1px #ddd;
}

.mainNav li.disabled span {
  opacity: 0.5;
  filter: alpha(opacity=50);
}


/* Alerts & Notes */

.notesList, .alertsList {
  margin-bottom: 15px;
  padding: 2px 15px 10px;
}

.notesList li, .alertsList li {
  margin: 10px 0 -5px;
  padding: 13px 0 8px;
  border-top: solid 1px #ddd;
  overflow: hidden;
}

.notesList li:first-child, .alertsList li:first-child {
  padding-top: 0;
  border-top: 0 none;
}

.notesList p, .alertsList p {
  margin: 0;
  padding: 5px 0 0;
}

.notesList h4, .notesList h4 a, .alertsList a {
  margin: 0;
  font: normal 16px 'Proxima Nova SemiBold', Arial, Helvetica;
  color: #777;
}


/* Alerts, Notes and Alerts List */ 

.notesList h4 span {
  float: right;
  font-family: 'Proxima Nova Regular';
  color: #999;
}

#notes .nanoScroller {
  height: 380px;
}

.alertsList {
  background: #f7f0f0;
  border: solid 1px #daa6a7;
  border-top: 0;
}

.alertsList li {
  padding: 9px 0 5px;
}

.alertsList a {
  display: block;
  padding-left: 25px;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: left center;
}

.alertsList a span {
  float: right;
  font: normal 14px 'Proxima Nova Regular', Arial, Helvetica;
}

.openOrders {
  margin-bottom: 15px;
  padding: 10px 10px 8px 38px;
  font: normal 18px/20px "Proxima Nova Semibold", Arial, Helvetica, Serif;
  background: #ffe9d3 url('../img/icon_open_orders.png') no-repeat 10px 10px;
  border: solid 1px #e5a984;
}

.openOrders a {
  color: #d76733;
}

.openOrders .right {
  font-size: 16px;
}



/**
 * Subscription
 */

/* Triple Play Content */

.TriplePlayContent {
  padding: 15px;
  background-color: #f9f9f9;
  overflow: hidden;
}

.TriplePlayContent .heading {
  margin: 0;
  padding: 10px 15px;
  font-family: 'Proxima Nova SemiBold';
  font-size: 20px;
}

.TriplePlayContent .heading span {
  float: right;
}


/* Equal Heights Container */

.equalHeightsContainer {
  clear: both;
}

.equalHeightsContainer .heading {
  margin: 0 -1px;
}

.equalHeightsContainer, .TriplePlayPackages {
  /* Fixes Separator Line overflow */
  overflow: hidden;
}

.innerRow {
  position: relative;
  margin: 0 -1px;
  min-height: 15px;
  background: #f9f9f9;
  overflow: hidden;
}

/* Triple Play Packages */

.TriplePlayPackages, .CustomerInterest {
  display: table;
  width: 100%;
}

.TriplePlayPackages:before, .CustomerInterest:before {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 10px;
  background: #fff;
  overflow: hidden;
  z-index: 999;
}

.TriplePlayPackages h2 {
  position: relative;
  margin: 0;
  padding: 6px 15px 12px;
  font: normal 20px 'Proxima Nova SemiBold', Arial, Helvetica, serif;
  background-color: #f7f7f7;
}

.CustomerInterest h2 {
  margin: 12px 15px 0;
  padding-bottom: 10px;
  font: normal 18px 'Proxima Nova Regular', Arial, Helvetica, serif;
  text-transform: uppercase;
  border-bottom: solid 1px #ccc;
}

.CustomerInterest label {
  margin-bottom: 8px;
  font: normal 16px 'Proxima Nova Regular', Arial, Helvetica, serif;
  color: #777;
}

.CustomerInterest textarea {
  margin-bottom: 15px;
  padding: 8px;
  width: 92%;
  min-height: 120px;
  font-size: 16px;
  background: #f8f8f8;
}

.CustomerInterest .ActionButtons {
  display: inline-block;
  margin-left: 5px;
  padding: 0;
  vertical-align: middle;
}

.TriplePlayPackages h2 label {
  font-weight: normal;
}

.TriplePlayPackages h2 span {
  display: inline-block;
  margin-left: 5px;
  font-family: 'Proxima Nova SemiBold', Arial, Helvetica, serif;
  font-size: 14px;
  text-transform: uppercase;
}

.TriplePlayPackages h2 span.right {
  font: 14px 'Proxima Nova SemiBold', Arial, Helvetica, serif;
  line-height: 28px;
  text-transform: none;
}

.TriplePlayPackages h2 .ServicePhoneNumber {
  letter-spacing: -1.5px;
}

.TriplePlayPackages h3 {
  margin-bottom: 6px;
  font: normal 16px 'Proxima Nova Semibold', Arial, Helvetica, Serif;
  color: #666;
}

.TriplePlayPackages > ul, .CustomerInterest > ul {
  display: table-row;
}

.TriplePlayPackages > ul > li, .CustomerInterest > ul > li {
  display: table-cell;
  position: relative;
  float: none;
  background: #fff;
  border: 0;
}

.TriplePlayPackages > ul > li:first-child {
  border-left: 0;
}

.TriplePlayPackages > ul > li .pane:before {
  /* Separator Line between Panes */
  content: '';  
  position: absolute;
  top: 10px;
  left: 0;
  width: 1px;
  height: 1000px;
  background :#eee;
  z-index: 10;
}

.TriplePlayPackages > ul > li:first-child .pane:before {
  display: none;
}

.TriplePlayPackages .pane, .CustomerInterest .pane, .TriplePlayOffers .pane {
  position: relative;
  padding: 10px 15px;
  color: #666;
  background :#fff;
}

.TriplePlayPackages .pane li, .TriplePlayOffers .pane li {
  position: relative;
  margin-bottom: 4px;
}

.TriplePlayPackages .pane li .includedTierPrice {
  position: absolute;
  top: -4px;
  right: 1px;
  margin-top: -10px;
  width: 10px;
  height: 23px;
  font-size: 10px;
  border-right: solid 1px #ccc;
  border-bottom: solid 1px #ccc;
}

.TriplePlayPackages .pane li .includedTierPrice.first {
  margin-top: 0;
  height: 20px;
}

.TriplePlayPackages .pane hr {
  margin: 8px 0;
  height: 1px;
  background-color: #fff;
  border: none;
  border-top: solid 1px #eee;
  box-shadow: none;
}

.TriplePlayExtra, .TriplePlayOffers  {
  float: left;
  margin-top: 10px;
  width: 100%;
}

.TriplePlayOffers .TriplePlayPackages .pane {
  padding-bottom: 0;
}

.TriplePlayOffers h6 {
  margin: 0;
  line-height: 1;
}

.TriplePlayOffers h6 span:first-child {
  font-weight: normal;
  color: #999;
}

.TriplePlayTotal {
  margin-bottom: 10px;
  padding-top: 0 !important;
  text-align: center;
}

.TriplePlayTotal li {
  display: inline-block;
  margin: 0 5px;
  vertical-align: middle;
}

.TriplePlayTotal li h5 {
  font: normal 22px 'Proxima Nova Light', Arial, Helvetica, Serif;
}

.TriplePlayTotal li h5 span {
  font-size: 15px;
  color: #777;
}

.TriplePlayTotal li .alert {
  font-family: 'Proxima Nova Regular';
  font-size: 48px;
}

.suspended {
  margin: 5px 0;
  padding-left: 25px;
  min-height: 40px;
  font:normal 18px "Proxima Nova Semibold", Arial, Helvetica, Serif;
  letter-spacing: 0.5px;
  color: #b80800;
  background: url('data:image/png;base64,') no-repeat;
}



