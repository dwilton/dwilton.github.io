/**
 * Lightbox / Modal styles to substitute rich.css and style.css
 */

.modalPage {
  display: none;
  position: absolute;
  top: 0; 
  left: 0;
  width: 100%;
  height: 100%;
}

.modal {
  padding: 0;
  width: 100%;
  height: 100%;
  background-color: #efefef;
  border: none;
  z-index: 1000;
}

.modalMask {
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 1003;
}

.modalMask .modalBackground {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.4;
  z-index: 1003;
  filter: alpha(Opacity=40);
}

.closeIcon {
  display: block;
  position: absolute;
  top: 8px;
  right: 8px;
  width: 12px;
  height: 13px;
  text-align: left;
  text-indent: -9999em;
  background: transparent url('data:image/png;base64,') no-repeat 0 0;
  z-index: 10;
}

.updateDetailsLightBox {
  position: absolute;
  width: 1080px;
  height: 810px;
  z-index: 1006;
}